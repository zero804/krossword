<?xml version="1.0" encoding="UTF-8"?>

<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-acrosslite-puz">
    <comment>AcrossLite Crossword</comment>
    <glob pattern="*.puz"/>
  </mime-type>
  <mime-type type="application/x-krosswordpuzzle">
    <sub-class-of type="application/xml"/>
    <comment>KrossWordPuzzle Crossword</comment>
    <glob pattern="*.kwp"/>
  </mime-type>
  <mime-type type="application/x-krosswordpuzzle-compressed">
    <sub-class-of type="application/zip"/>
    <comment>Compressed KrossWordPuzzle Crossword</comment>
    <glob pattern="*.kwpz"/>
  </mime-type>
</mime-info>